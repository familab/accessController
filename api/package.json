{
  "name": "accessController",
  "version": "2.0.0",
  "private": true,
  "description": "Node Library to manage access control hardware ",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/gulp --harmony",
    "start": "./node_modules/.bin/nodemon --harmony index.js",
    "migrate": "./node_modules/.bin/sequelize db:migrate"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/familab/accessController.git"
  },
  "keywords": [
    "nfc",
    "udp",
    "access control"
  ],
  "author": [
    "Lance Hudson <lance@lancehudson.com> (http://lancehudson.com)",
    "Daniel Grove <danny@drgrovellc.com>  (http://drgrovellc.com)"
  ],
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/familab/accessController/issues"
  },
  "homepage": "https://github.com/familab/accessController#readme",
  "dependencies": {
    "debug": "^2.2.0",
    "jade": "^1.11.0",
    "koa": "^1.0.0",
    "koa-better-ratelimit": "^2.1.2",
    "koa-body": "^1.3.0",
    "koa-compress": "^1.0.8",
    "koa-conditional-get": "^1.0.3",
    "koa-errorhandler": "^0.1.1",
    "koa-etag": "^2.0.0",
    "koa-helmet": "^0.2.0",
    "koa-logger": "^1.3.0",
    "koa-mount": "^1.3.0",
    "koa-response-time": "^1.0.2",
    "koa-router": "^5.2.1",
    "koa-static": "^1.4.9",
    "nodemon": "^1.7.1",
    "sequelize": "^3.9.0",
    "sequelize-cli": "^1.9.1",
    "sqlite3": "^3.0.8",
    "swagger-jsdoc": "^1.1.1",
    "swagger-ui": "^2.1.3"
  },
  "jscsConfig": {
    "preset": "node-style-guide",
    "esnext": true
  },
  "jshintConfig": {
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "freeze": true,
    "indent": 2,
    "newcap": true,
    "quotmark": "single",
    "maxdepth": 3,
    "maxstatements": 15,
    "maxlen": 80,
    "eqnull": true,
    "funcscope": true,
    "node": true,
    "esnext": true,
    "noyield": true,
    "mocha": true
  },
  "engines": {
    "node": ">=0.12"
  },
  "devDependencies": {
    "gulp": "^3.9.0",
    "gulp-jscs": "^3.0.0",
    "gulp-jscs-stylish": "^1.1.0",
    "gulp-jshint": "^1.11.2",
    "gulp-mocha": "^2.1.3",
    "gulp-util": "^3.0.6",
    "jshint-stylish": "^2.0.1",
    "mocha": "^2.2.5",
    "should": "^7.0.2",
    "supertest": "^1.0.1"
  }
}
